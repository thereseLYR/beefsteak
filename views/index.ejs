  <div class="text-center mx-2 my-5">
    <h1 id="elapsedTime" style="font-size: 72pt;">25 : 00</h1>
    <div id="functionButtons">
      <button id="startButton" class="btn btn-danger disabled" disabled>
        Start
      </button>
    </div>
  </div>

  <div class="d-flex justify-content-center mx-2">
    <form action="/tasklist/list" method="POST">
      <p class="text-center text-muted">Create a Tasklist, and then start your timer.</p>

      <div class="my-1">
        <label for="list-name"><b>Tasklist Name</b></label><br />
        <input type="text" class="form-control" placeholder="Studying for 30.008 Test" name="list-name" required />
      </div>

      <div class="my-1">
        <label for="list-description"><b>Tasklist Description</b></label><br />
        <textarea class="form-control" rows="5" cols="60" name="list-description" placeholder="Optional"></textarea>
        <!-- <input type="text" placeholder="Optional" name="list-description" /> -->
      </div>

      <div class="my-1">
        <label for="task-1"><b>Task 1</b></label><br />
        <input type="text" class="form-control" placeholder="Summarize last lesson" name="task-1" required />
      </div>

      <div class="my-1">
        <label for="task-2"><b>Task 2</b></label><br />
        <input type="text" class="form-control" placeholder="Draw and Label Diagrams" name="task-2" required />
      </div>

      <div class="my-1">
        <label for="task-3"><b>Task 3</b></label><br />
        <input type="text" class="form-control" placeholder="Create section summary" name="task-3" required />
      </div>

      <div class="d-grid gap-2 my-2">
        <button class="btn btn-success" type="submit">Save Changes</button>
      </div>
    </form>
  </div>

